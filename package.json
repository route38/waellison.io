  "react-dotenv": {
    "whitelist": [
      "GITHUB_API_KEY"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "\\.(less|css|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "resetMocks": true,
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "usePathForSuiteName": true,
          "outputDirectory": "./coverage"
        }
      ]
    ],
    "testResultsProcessor": "jest-junit"
  }
